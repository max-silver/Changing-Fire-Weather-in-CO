```{r}
#pvals re21 w/ adj
pvaltab <- pvals_adj 

for (i in 1:12) {
  for (j in 1:5) {
    pvaltab[i,j] = ifelse(pvaltab[i,j] == 'NaN', 'FAIL', pvaltab[i,j])
    pvaltab[i,j] = cell_spec(pvaltab[i,j], "latex", bold = ifelse(pvaltab[i,j] < 0.05 | pvaltab[i,j] == 'FAIL', T, F), italic = ifelse(pvaltab[i,j] < 0.1 & pvaltab[i,j] > 0.05, T, F), color = case_when(pvaltab[i,j] == 'FAIL' ~ 'orange', pvaltab[i,j] > .1 ~ 'black', pvaltab[i,j] < .1 & pval_sign.adj[i,j] == -1 ~ 'blue', pvaltab[i,j] < .1 & pval_sign.adj[i,j] == 1  ~ 'red'))
  }}

knitr::kable(pvaltab, format="latex", booktabs = T, escape = F, align = 'c', linesep = "")
```

