```{r results = FALSE}
pval_sign.adj[1,1] <- abs(mkTrend(jan_HDWI.obs$HDW_5)$tau[1])/mkTrend(jan_HDWI.obs$HDW_5)$tau[1]
pval_sign.adj[2,1] <- abs(mkTrend(feb_HDWI.obs$HDW_5)$tau[1])/mkTrend(feb_HDWI.obs$HDW_5)$tau[1]
pval_sign.adj[3,1] <- abs(mkTrend(mar_HDWI.obs$HDW_5)$tau[1])/mkTrend(mar_HDWI.obs$HDW_5)$tau[1]
pval_sign.adj[4,1] <- abs(mkTrend(apr_HDWI.obs$HDW_5)$tau[1])/mkTrend(apr_HDWI.obs$HDW_5)$tau[1]
pval_sign.adj[5,1] <- abs(mkTrend(may_HDWI.obs$HDW_5)$tau[1])/mkTrend(may_HDWI.obs$HDW_5)$tau[1]
pval_sign.adj[6,1] <- abs(mkTrend(jun_HDWI.obs$HDW_5)$tau[1])/mkTrend(jun_HDWI.obs$HDW_5)$tau[1]
pval_sign.adj[7,1] <- abs(mkTrend(jul_HDWI.obs$HDW_5)$tau[1])/mkTrend(jul_HDWI.obs$HDW_5)$tau[1]
pval_sign.adj[8,1] <- abs(mkTrend(aug_HDWI.obs$HDW_5)$tau[1])/mkTrend(aug_HDWI.obs$HDW_5)$tau[1]
pval_sign.adj[9,1] <- abs(mkTrend(sep_HDWI.obs$HDW_5)$tau[1])/mkTrend(sep_HDWI.obs$HDW_5)$tau[1]
pval_sign.adj[10,1] <- abs(mkTrend(oct_HDWI.obs$HDW_5)$tau[1])/mkTrend(oct_HDWI.obs$HDW_5)$tau[1]
