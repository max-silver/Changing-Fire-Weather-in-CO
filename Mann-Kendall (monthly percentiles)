```{r}
library(dplyr)
library(kableExtra)
library(ggplot2)
library(Kendall)
library(fume)
```

```{r}
obsHDWI <- read.csv('OBSERVATIONAL-HDWI-TITLE.csv')
reHDWI <- read.csv('REANALYSIS-HDWI-TITLE.csv')
```

```{r}
q95 <- function(x) {
  return(quantile(x, .95))
  }
q75 <- function(x) {
  return(quantile(x, .75))
}
q25 <- function(x) {
  return(quantile(x, .25))
  }
q05 <- function(x) {
  return(quantile(x, .05))
}
```

```{r}
max_HDWI.obs <- obsHDWI %>% 
  mutate(month = as.numeric(month))%>%
  mutate(year = as.numeric(year))%>%
  group_by(month, year) %>%
  summarize_at('HDW', max) 

p95_HDWI.obs <- obsHDWI %>% 
  mutate(month = as.numeric(month))%>%
  group_by(month, year) %>%
  summarize_at('HDW', q95) 

p75_HDWI.obs <- obsHDWI %>% 
  mutate(month = as.numeric(month))%>%
  group_by(month, year) %>%
  summarize_at('HDW', q75)

med_HDWI.obs <- obsHDWI %>% 
  mutate(month = as.numeric(month))%>%
  group_by(month, year) %>%
  summarize_at('HDW', median) 

p25_HDWI.obs <- obsHDWI %>% 
  mutate(month = as.numeric(month))%>%
  group_by(month, year) %>%
  summarize_at('HDW', q25) 

p05_HDWI.obs <- obsHDWI %>% 
  mutate(month = as.numeric(month))%>%
  group_by(month, year) %>%
  summarize_at('HDW', q05)

min_HDWI.obs <- obsHDWI %>% 
  mutate(month = as.numeric(month))%>%
  group_by(month, year) %>%
  summarize_at('HDW', min) 
```

```{r}
HDWI.obs_pctl <- max_HDWI.obs %>% 
  ungroup() %>%
  mutate(HDW_min = min_HDWI.obs$HDW) %>%
  mutate(HDW_5 = p05_HDWI.obs$HDW) %>%
  mutate(HDW_25 = p25_HDWI.obs$HDW) %>%
  mutate(HDW_50 = med_HDWI.obs$HDW) %>%
  mutate(HDW_75 = p75_HDWI.obs$HDW) %>%
  mutate(HDW_95 = p95_HDWI.obs$HDW) %>%
  mutate(HDW_max = max_HDWI.obs$HDW) %>%
  mutate(Year = year) %>%
  dplyr::select(-HDW, -year)
```

```{r}
max_HDWI.re <- reHDWI %>% 
  mutate(month = as.numeric(month))%>%
  mutate(year = as.numeric(year))%>%
  group_by(month, year) %>%
  summarize_at('HDW', max) 

p95_HDWI.re <- reHDWI %>% 
  mutate(month = as.numeric(month))%>%
  group_by(month, year) %>%
  summarize_at('HDW', q95) 

p75_HDWI.re <- reHDWI %>% 
  mutate(month = as.numeric(month))%>%
  group_by(month, year) %>%
  summarize_at('HDW', q75)

med_HDWI.re <- reHDWI %>% 
  mutate(month = as.numeric(month))%>%
  group_by(month, year) %>%
  summarize_at('HDW', median) 

p25_HDWI.re <- reHDWI %>% 
  mutate(month = as.numeric(month))%>%
  group_by(month, year) %>%
  summarize_at('HDW', q25) 

p05_HDWI.re <- reHDWI %>% 
  mutate(month = as.numeric(month))%>%
  group_by(month, year) %>%
  summarize_at('HDW', q05)

min_HDWI.re <- reHDWI %>% 
  mutate(month = as.numeric(month))%>%
  group_by(month, year) %>%
  summarize_at('HDW', min) 
```

```{r}
HDWI.re_pctl <- max_HDWI.re %>% 
  ungroup() %>%
  mutate(HDW_min = min_HDWI.re$HDW) %>%
  mutate(HDW_5 = p05_HDWI.re$HDW) %>%
  mutate(HDW_25 = p25_HDWI.re$HDW) %>%
  mutate(HDW_50 = med_HDWI.re$HDW) %>%
  mutate(HDW_75 = p75_HDWI.re$HDW) %>%
  mutate(HDW_95 = p95_HDWI.re$HDW) %>%
  mutate(HDW_max = max_HDWI.re$HDW) %>%
  mutate(Year = year) %>%
  dplyr::select(-HDW, -year)
```

```{r}
jan_HDWI.obs <- filter(HDWI.obs_pctl, month==1)
feb_HDWI.obs <- filter(HDWI.obs_pctl, month==2)
mar_HDWI.obs <- filter(HDWI.obs_pctl, month==3)
apr_HDWI.obs <- filter(HDWI.obs_pctl, month==4)
may_HDWI.obs <- filter(HDWI.obs_pctl, month==5)
jun_HDWI.obs <- filter(HDWI.obs_pctl, month==6)
jul_HDWI.obs <- filter(HDWI.obs_pctl, month==7)
aug_HDWI.obs <- filter(HDWI.obs_pctl, month==8)
sep_HDWI.obs <- filter(HDWI.obs_pctl, month==9)
oct_HDWI.obs <- filter(HDWI.obs_pctl, month==10)
nov_HDWI.obs <- filter(HDWI.obs_pctl, month==11)
dec_HDWI.obs <- filter(HDWI.obs_pctl, month==12)
```

```{r}
jan_HDWI.re <- filter(HDWI.re_pctl, month==1)
feb_HDWI.re <- filter(HDWI.re_pctl, month==2)
mar_HDWI.re <- filter(HDWI.re_pctl, month==3)
apr_HDWI.re <- filter(HDWI.re_pctl, month==4)
may_HDWI.re <- filter(HDWI.re_pctl, month==5)
jun_HDWI.re <- filter(HDWI.re_pctl, month==6)
jul_HDWI.re <- filter(HDWI.re_pctl, month==7)
aug_HDWI.re <- filter(HDWI.re_pctl, month==8)
sep_HDWI.re <- filter(HDWI.re_pctl, month==9)
oct_HDWI.re <- filter(HDWI.re_pctl, month==10)
nov_HDWI.re <- filter(HDWI.re_pctl, month==11)
dec_HDWI.re <- filter(HDWI.re_pctl, month==12)
