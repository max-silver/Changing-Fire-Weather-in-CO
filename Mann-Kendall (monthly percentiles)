```{r}
pval_sign.adj[1,5] <- abs(mkTrend(jan_HDWI.obs$HDW_95)$tau[1])/mkTrend(jan_HDWI.obs$HDW_95)$tau[1]
pval_sign.adj[2,5] <- abs(mkTrend(feb_HDWI.obs$HDW_95)$tau[1])/mkTrend(feb_HDWI.obs$HDW_95)$tau[1]
pval_sign.adj[3,5] <- abs(mkTrend(mar_HDWI.obs$HDW_95)$tau[1])/mkTrend(mar_HDWI.obs$HDW_95)$tau[1]
pval_sign.adj[4,5] <- abs(mkTrend(apr_HDWI.obs$HDW_95)$tau[1])/mkTrend(apr_HDWI.obs$HDW_95)$tau[1]
pval_sign.adj[5,5] <- abs(mkTrend(may_HDWI.obs$HDW_95)$tau[1])/mkTrend(may_HDWI.obs$HDW_95)$tau[1]
pval_sign.adj[6,5] <- abs(mkTrend(jun_HDWI.obs$HDW_95)$tau[1])/mkTrend(jun_HDWI.obs$HDW_95)$tau[1]
